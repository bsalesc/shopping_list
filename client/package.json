{
  "name": "shopping-list",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:o": "ng serve -o",
    "build": "ng build",
    "test": "ng test",
    "tslint": "./node_modules/.bin/tslint --project ./",
    "deploy": "yarn build --prod --aot && yarn ngsw && yarn ngsw:copy",
    "deploy:app": "yarn build --prod --aot --base-href ./ --output-path ../mobile/www",
    "test:headless": "yarn tslint && ng test --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "ngsw": "node_modules/.bin/ngsw-config dist/shoppingList src/ngsw-config.json",
    "ngsw:test": "yarn build --prod && yarn ngsw && yarn ngsw:copy && http-server -p 8888 ./dist/shoppingList",
    "ngsw:copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/shoppingList",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@angular/service-worker": "^7.2.3",
    "bootstrap": "^4.1.2",
    "core-js": "^2.6.3",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.21",
    "codelyzer": "~4.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "protractor": "~5.4.2",
    "terser": "3.16",
    "ts-node": "~8.0.2",
    "tslint": "^5.12.1",
    "typescript": "3.2.4"
  }
}
